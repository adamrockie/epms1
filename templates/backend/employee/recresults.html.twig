{% extends "backend/patials/master.html" %}


{% block title %} {{title}} {% endblock %}

    {% block headscripts %}

    <link rel="stylesheet" href="assets/css/bootstrap-datetimepicker.min.css">
    <!-- Datatable CSS -->
    <link rel="stylesheet" href="assets/css/dataTables.bootstrap4.min.css">

    <style>
        /* Preview */
        .preview{
            width: 100px;
            height: 100px;
            border: 1px solid black;
            margin: 0 auto;
            background: white;
        }
        
        .preview img{
            display: none;
        }
    </style>

    {% endblock %}

{% block content %}

<div id="fakeloader-overlay" class="visible incoming">
    <div class="loader-wrapper-outer">
      <div class="loader-wrapper-inner">
        <div class="loader"></div>
      </div>
    </div>
</div>

<!-- Page Content -->
<div class="content container-fluid"  id="mcontents">
    
    <!-- Page Header -->
    <div class="page-header">
        <div class="row align-items-center">
            <div class="col">
                <h3 class="page-title">Inventory</h3>
                <ul class="breadcrumb">
                    <li class="breadcrumb-item"><a href="dashboard">Dashboard</a></li>
                    <li class="breadcrumb-item"><a href="inventory">Inventory</a></li>
                    <li class="breadcrumb-item active">Inventory Report Results</li>
                </ul>
            </div>
        </div><br>

    </div>
    <!-- /Page Header -->
    <div>
        <button class="btn btn-primary" onclick="history.back()"> <i class="la la-chevron-left"></i>  Back to Search</button>
    </div><br><hr>
    <div class="row">
        <div class="col-md-12">
            <div class="table-responsive">
                <table class="table table-striped custom-table datatable" id="staff_table">
                    <thead>
                        <tr>
                            
                            <th>Contract Name</th>
                            <th>Contractor</th>
                            <th>Email</th>
                            <th>Status</th>
                            <th>Date Issued</th>
                            <th class="text-end no-sort">Action</th>
                        </tr>
                    </thead>
                        <tbody>
                    {% for items in responses %}
                            <tr>

                                <td>{{items.contract}}</td>
                                <td>{{items.contractor}}</td>
                                <td>{{items.email}}</td>
                                <td>{{items.status}}</td>
                                <td>{{items.date}}</td>
                                <td class="text-end">
                                    <div class="dropdown dropdown-action">
                                        <a href="#" class="action-icon dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false"><i class="material-icons">more_vert</i></a>
                                        <div class="dropdown-menu dropdown-menu-right">
                                            <!--a class="dropdown-item" href="javascript:void(0);"  onclick="add_receivable('{{receivable}}')"><i class="fa fa-eye m-r-5"></i> More Details</a-->
                                            <a class="dropdown-item" href="#" data-bs-toggle="modal" data-bs-target="#edit_receivable" onclick="edit_receivable('{{items.id}}')"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                            <!--a class="dropdown-item" href="#" data-bs-toggle="modal" data-bs-target="#delete_employee" onclick="delete_warning('{{employee.ippis}}');"><i class="fa fa-trash-o m-r-5"></i> Delete</a-->
                                        </div>
                                    </div>
                                </td>
                            </tr>

                    {% endfor %}
                        </tbody>
                </table>

            {#% include '/backend/employee/pagination.html.twig'  08131956314 1960 %#}
                           
            </div>
        </div>
    </div>
</div>
<!-- /Page Content -->


{% endblock %}

{% block jsscript %} 

<!-- Datetimepicker JS -->
<script src="assets/js/moment.min.js"></script>
<script src="assets/js/bootstrap-datetimepicker.min.js"></script>
<!-- Datatable JS -->
<script src="assets/js/jquery.dataTables.min.js"></script>
<script src="assets/js/dataTables.bootstrap4.min.js"></script>
<!-- Select 2 -->
<script src="assets/backend/js/select2.full.min.js"></script>

<script>

    view_profile = function(id){
        //alert(id);
            window.location.href = "profile/"+id;
    }   

    $(document).ready(
        function() {
            window.FakeLoader.init( { auto_hide: true } );
        }
    );

</script>

<script src="templates/backend/employee/inventory.js"></script>

{% endblock %}